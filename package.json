    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev:server": "npm run build && cross-env NODE_ENV=development sls offline start --stage offline",
        "build": "rm -rf spec-and-routes && rm -rf dist && rm -rf .build && rm -rf tsoa &&  mkdir -p ./src/generated && mkdir -p ./src/generated/routes && mkdir -p ./src/generated/swagger && tsoa spec-and-routes && tsc && npm run build:clients",
        "build:clients": "rm -rf ./clients &&  cd ./tools/openapi && openapi-generator-cli generate -i ../../src/generated/swagger/swagger.json -c ./config.json -o ../../client",
        "start": "node build/src/server.js"
    },
